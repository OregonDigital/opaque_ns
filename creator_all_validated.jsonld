{
"@id": "http://opaquenamespace.org/ns/creator/XianyuShu",
"@type": "PersonalName",
"definedBy": {
"@id": "http://opaquenamespace.org/VOCAB_PLACEHOLDER.nt"
},
"issued": {
"@value": "2014-07-10",
"@type": "date"
